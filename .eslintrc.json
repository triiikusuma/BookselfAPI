{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "import/no-extraneous-dependencies": 0,
        "no-console": "off",
        "no-underscore-dangle": "off",
        "no-restricted-globals": "off",
        "linebreak-style": "off",
        "no-unused-vars": "off",
        "indent": "off",
        "comma-dangle": "off"
    }
}
